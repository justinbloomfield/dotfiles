#!/bin/sh
EMOTE=$1

case $EMOTE in
    yeahnah)
        xdotool getwindowfocus windowfocus --sync sleep 0.1 type 'xahrF.adbad '
        ;;
    cry)
        xdotool getwindowfocus windowfocus --sync sleep 0.1 type 'xahrJpf '
        ;;
    praise)
        xdotool getwindowfocus windowfocus --sync sleep 0.1 type 'xahrLpaco. '
        ;;
    clown)
        xdotool getwindowfocus windowfocus --sync sleep 0.1 type 'xahrJnr,bcb '
        ;;
    rage)
        xdotool getwindowfocus windowfocus --sync sleep 0.1 type 'xahrPai. '
        ;;
    maker)
        xdotool getwindowfocus windowfocus --sync sleep 0.1 type 'xahrMat.p '
        ;;
    8bit)
        xdotool getwindowfocus windowfocus --sync sleep 0.1 type 'xahr8xcy '
        ;;
    quasarface)
        xdotool getwindowfocus windowfocus --sync sleep 0.1 type 'xahr"gaoapuaj. '
        ;;
    graphics)
        xdotool getwindowfocus windowfocus --sync sleep 0.1 type 'xahrIpaldcjo '
        ;;
    art)
        xdotool getwindowfocus windowfocus --sync sleep 0.1 type 'xahrApy '
        ;;
    ponder)
        xdotool getwindowfocus windowfocus --sync sleep 0.1 type 'xahrLrbe.p '
        ;;
    bidet)
        xdotool getwindowfocus windowfocus --sync sleep 0.1 type 'xahrXce.y '
        ;;
    kisses)
        xdotool getwindowfocus windowfocus --sync sleep 0.1 type 'xahrTcoo.o '
        ;;
    sad)
        xdotool getwindowfocus windowfocus --sync sleep 0.1 type 'xahrOae '
        ;;
    *)
        
esac
