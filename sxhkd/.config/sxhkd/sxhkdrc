alt + Tab
  waitron window_cycle

alt + shift + Tab
  waitron window_rev_cycle

super + d
	thinglaunch

XF86AudioRaiseVolume
    pactl set-sink-volume 0 +5%

XF86AudioLowerVolume
    pactl set-sink-volume 0 -5%

XF86AudioMute
    pactl set-sink-mute 0 toggle

XF86AudioPlay
    mpc toggle

XF86AudioStop
    mpc stop

super + Escape
	pkill -USR1 -x sxhkd
